<% if edit_enabled.present? %>
  <input type="hidden" name="review[rating]" class="rating rating-value" value="<%= stars %>" data-fractions="2" data-filled="fa fa-star fa-2x" data-empty="fa fa-star-o fa-2x"/>
<% else %>
  <input type="hidden" class="rating" disabled="disabled" value="<%= stars %>" data-fractions="2" data-filled="fa fa-star fa-2x" data-empty="fa fa-star-o fa-2x"/>
<% end %>
