                  <%= email_field(:seller, :address9, class: 'form-control') %>
                </div>
              </div>
              <div class="form-group">
                <label class="control-label col-sm-3" for="seller_address10">Seller 10 email</label>
                <div class="col-sm-9>">
                  <%= email_field(:seller, :address10, class: 'form-control') %>
                </div>
              </div>
            </div>
          </div>
        </div>
        <div class="modal-footer">
          <button type="button" class="btn btn-success" id="invite-sellers-submit" data-dismiss="modal">Invite</button>
          <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
        </div>
      </form>
    </div>
  </div>
</div>
