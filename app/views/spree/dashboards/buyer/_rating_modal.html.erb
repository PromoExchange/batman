<div aria-hidden="true" aria-labelledby="basicModal" class="modal" data-key="<%=spree_current_user.spree_api_key%>" id="rating-seller" role="dialog" tabindex="-1">
  <div class="modal-dialog text-center">
    <div class="modal-content">
      <div class="modal-header">
       <h4> <strong>Rate Service</strong></h4>
      </div>
      <div class="modal-body">
        <%= hidden_field_tag id='rating-auction-id', 0 %>
        <%= hidden_field_tag id='select-rating'  %>
        <div class="form-group" id='rating'>
          <%= render 'spree/dashboards/shared/stars', stars: '0', edit_enabled: true %>
        </div>
        <div class="form-group">
          <p>Comment:</p>
          <textarea id="rating-review" class="form-control" rows="5"></textarea>
        </div>
        <div class="form-group" id="rating-submit">
          <button class="btn btn-success" data-status="submit" >Submit</button>
          <span>or</span>
          <button class="btn btn-default" data-status="skip">Skip</button>
        </div>
      </div>
    </div>
  </div>
</div>
