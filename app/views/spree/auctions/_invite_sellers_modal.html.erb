                <label class="control-label col-sm-3" for="seller_address3">Seller 3 email</label>
                <div class="col-sm-9>">
                  <%= email_field(:seller, :address3, class: 'form-control') %>
                </div>
              </div>
              <div class="form-group">
                <label class="control-label col-sm-3" for="seller_address4">Seller 4 email</label>
                <div class="col-sm-9>">
                  <%= email_field(:seller, :address4, class: 'form-control') %>
                </div>
              </div>
              <div class="form-group">
