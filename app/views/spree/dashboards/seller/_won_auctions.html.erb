<% content_for :page_title do %>
<%= Spree.t(:won_auctions) %>
<% end %>

<div class="tab-pane scrolling <%= 'active' if @tab == 'won_auction' %>" data-id="<%= spree_current_user.id %>" data-key="<%= spree_current_user.spree_api_key %>" id="seller-won-auction">
  <div class="panel panel-default">
    <div class="panel-body">
      <table class="table table-striped" data-id="<%= spree_current_user.id %>" data-key="<%= spree_current_user.spree_api_key %>" id="seller-won-auction-table">
        <thead>
          <tr>
            <th><%= Spree.t(:auction_hash) %></th>
            <th><%= Spree.t(:product) %></th>
            <th><%= Spree.t(:date) %></th>
            <th><%= Spree.t(:quantity) %></th>
            <th><%= Spree.t(:status) %></th>
            <th><%= Spree.t(:px_fee) %></th>
            <th>&nbsp;</th>
          </tr>
        </thead>
        <tbody>
          <tr>
            <td class='text-center' colspan='7'>
              <i class='fa fa-spinner fa-pulse fa-3x'></i>
            </td>
          </tr>
        </tbody>
      </table>
    </div>
  </div>
</div>
<%= render partial: 'spree/dashboards/seller/pay_modal' %>
<%= render partial: 'spree/dashboards/seller/tracking_modal' %>
<%= render partial: 'spree/dashboards/tracking_information_modal' %>
<%= render partial: 'spree/dashboards/seller/claim_payment_modal' %>
<%= render partial: 'spree/dashboards/seller/upload_proof_modal' %>
<%= render partial: 'spree/dashboards/seller/proof_feedback_modal' %>
