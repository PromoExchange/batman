.row
  .col-md-6
    = f.field_container :name, class: ['form-group'] do
      = f.label :name, "Name"
      span.required *
      = f.text_field :name, :class => 'form-control'
      = f.error_message_on :name
  .col-md-6
    = f.field_container :dc_acct_num, class: ['form-group'] do
      = f.label :dc_acct_num, "DC Account Number"
      span.required *
      = f.text_field :dc_acct_num, :class => 'form-control'
      = f.error_message_on :dc_acct_num
