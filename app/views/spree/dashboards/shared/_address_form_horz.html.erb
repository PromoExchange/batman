<div class="form-group">
  <label class="control-label" for="pxaddress_company">Company</label> <br />
  <input type="text" id="address_edit_company">
</div>
<div class="form-group">
  <label class="control-label" for="pxaddress_firstname">First Name</label> <br />
  <input type="text" id="address_edit_firstname">
</div>
<div class="form-group">
  <label class="control-label" for="pxaddress_lastname">Last Name</label> <br />
  <input type="text" id="address_edit_lastname">
</div>
<div class="form-group">
  <label class="control-label" for="pxaddress_address1">Address</label> <br />
  <input type="text" id="address_edit_address1" maxlength="50" size="50">
</div>
<div class="form-group">
  <label class="control-label" for="pxaddress_address2">Address (contd.)</label> <br />
  <input type="text" id="address_edit_address2" maxlength="50" size="50">
</div>
<div class="form-group">
  <label class="control-label" for="pxaddress_city" class="col-sm-3">City</label> <br />
  <input type="text" id="address_edit_city" maxlength="50" size="50">
</div>
<div class="form-group">
  <label for="pxaddress_state"><%= Spree.t(:state) %></label> <br />
  <select id="address_edit_state">
    <%= render partial: 'spree/pxusers/us_states' %>
  </select>
</div>
<div class="form-group">
  <label class="control-label" for="pxaddress_zipcode">Zip</label> <br />
  <input type="text" id="address_edit_zipcode">
</div>
<div class="form-group">
  <label class="control-label" for="pxaddress_phone">Phone</label> <br />
  <input type="text" id="address_edit_phone">
</div>
