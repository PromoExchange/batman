<div aria-hidden="true" aria-labelledby="basicModal" class="modal" data-key="<%=spree_current_user.spree_api_key%>" id="tracking-number" role="dialog" tabindex="-1">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <h4>Input Tracking Number</h4>
      </div>
      <div class="modal-body">
        <%= hidden_field_tag id='tracking-auction-id', 0 %>
        <div class="form-group">
          <input type="text" name="tracking_number" id="trackig-number" class="form-control" />
        </div>
        <div class="form-group">
          <button class="btn btn-success" id="tracking-submit" type="submit">Submit</button>
          <span>or</span>
          <button type="button" class="btn btn-default" id="tracking-close" data-dismiss="modal">Close</button>
        </div>
      </div>
    </div>
  </div>
</div>
