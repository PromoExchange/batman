                <div class="col-sm-9>">
                  <%= email_field(:seller, :address5, class: 'form-control') %>
                </div>
              </div>
              <div class="form-group">
                <label class="control-label col-sm-3" for="seller_address6">Seller 6 email</label>
                <div class="col-sm-9>">
                  <%= email_field(:seller, :address6, class: 'form-control') %>
